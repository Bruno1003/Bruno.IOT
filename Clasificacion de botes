import pandas as pd
from flask import Flask

app=Flask(__name__)

base=pd.read_excel("Clasificacion de botes.xlsx")

@app.route("/")
def Principal():
  return "Esta es una Api que te muestra canciones de aurora"

@app.route ("/Por_Numero/<Numero>")
def PorNumero (Numero):
  Numero=int(numero)
  fila=base[base["Numero"]==Numero]
@app.route("/Por_Geolocalizacion/<Longitud>/<Latitud>")
def PorGeolocalizacion(Longitud,Latitud):
  Longitud=float(Longitud)
  Latitud=float(Latitud)
  resultados=base[(base["Geolocalizacion"]>Longitud) & (base [Geolocalizacion]<Latitud)]
  resultados=str(resultados)
  return resultados

  if __name__=="__main__":
    app.run()
@app.route ("/Por_Numero/<Numero>")
def PorNumero(Numero):
  Numero=int(Numero)
  fila=base[base["Numero"]==Numero]
  respuestas=f"El bote {Numero} es {fila.loc[:,'Nombre']}"
  return respuesta

@app.route("/Por_Tipo/<Tipo>")
def PorTipo(Tipo):
  resultados=base[base["Tipo"]==Tipo]
  resultados=str(resultados)
  return resultados

